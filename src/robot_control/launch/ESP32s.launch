<?xml version="1.0"?>
<launch>

    <node name="BaseNODE" pkg="rosserial_python" type="serial_node.py"  output="screen" respawn="true" respawn_delay="10">
        <param name="port" value="/dev/ttyUSB1"/>
            <param name="baud" value="115200" />
    </node>
    <node name="HANDNODE" pkg="rosserial_python" type="serial_node.py"  output="screen" respawn="true" respawn_delay="10">
        <param name="port" value="/dev/ttyUSB0"/>
            <param name="baud" value="115200" />
    </node>

</launch>